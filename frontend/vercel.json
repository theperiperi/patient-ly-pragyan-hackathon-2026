{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    { "src": "/voice/(.*)", "dest": "api/index.py" },
    { "src": "/health", "dest": "api/index.py" }
  ]
}
